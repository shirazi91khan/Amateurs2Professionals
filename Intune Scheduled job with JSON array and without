In JSON. array
 
var tdaycount = 0;
var ydaycount = 0;
var ar = 0;
var tday = {
    arr: [{},{}]
};
var yday = {};
var gr = new GlideRecord("sys_import_set");
gr.addEncodedQuery('sys_created_onONToday@javascript:gs.beginningOfToday()@javascript:gs.endOfToday()^ORsys_created_onONYesterday@javascript:gs.beginningOfYesterday()@javascript:gs.endOfYesterday()'); // Checking for today's data
gr.addQuery('data_source', '59d993927721101077b22344681061c2'); // intune Device schedule
gr.query();
while (gr.next()) {
    var grmap = new GlideRecord("sys_import_set_run");
    grmap.addQuery("set", gr.sys_id);
    grmap.query();
    if (grmap.next()) {
        //if (grmap.state == 'cancelled' || grmap.state == 'incomplete') {
            tday.arr[ar].sysid = gr.sys_id + '';
            tday.arr[ar].create = gr.sys_created_on + '';
            tday.arr[ar].datasour = gr.getDisplayValue('data_source');
            tday.arr[ar].num = gr.number + '';
            tday.arr[ar].state = grmap.state + '';
            gs.info("get number " + grmap.number + " and Import " + gr.number);
        //}
        ar++;
    }
}
var tday = new JSON().encode(tday);
gs.info("get value of object  " + tday);
gs.eventQueue('intune.cancelsfor.2days', current, tday);
 
 
